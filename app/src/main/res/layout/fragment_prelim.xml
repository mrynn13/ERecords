<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PrelimFragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">
        <RelativeLayout
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:id="@+id/asc_tv_section">

            <TextView
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:text="Academic Standing Category"
                android:padding="5dp"
                android:id="@+id/asc_tv"
                android:background="#68f0ae"
                android:textSize="15sp"
                android:fontFamily="@font/custom_poppins"
                android:textColor="@color/black"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/asc_tv"
                android:orientation="vertical">




            </LinearLayout>



        </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/asc_tv_section"
        android:layout_alignParentRight="true"
        android:id="@+id/exam_tv_section">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
          android:id="@+id/exam_tv"
            android:text="Labs/Exams"
            android:background="#ef9999"
            android:padding="5dp"
            android:gravity="center"
            android:textSize="15sp"
            android:fontFamily="@font/custom_poppins"
            android:textColor="@color/black"/>

    </RelativeLayout>



    </RelativeLayout>

</FrameLayout>